SELECT 
    e.id,                  -- استخدام id وليس employee_code
    (SELECT id FROM beds LIMIT 1),  -- اختيار أول سرير موجود
    DATE('now'),
    'default'
FROM employees e
WHERE e.id NOT IN (
    SELECT employee_id
    FROM bed_assignments
);
</sql><current_tab id="0"/></tab_sql></sqlb_project>
